{
    "branches": ["master"],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        ["@semantic-release/github", {
            "assets": [
                {"path": "../../build/distributions/kcd_${nextRelease.version}.tar", "label": "Binary - Tar"},
                {"path": "../../build/distributions/kcd_${nextRelease.version}.zip", "label": "Binary - Zip"}
            ]
        }],
        ["@semantic-release/exec", {
          "successCmd" : "release-files.sh ${nextRelease.version}"
        }]
    ]
}
